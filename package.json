{
  "name": "@dnb-org/dnb-hugo-testcontent",
  "version": "1.0.2",
  "license": "MIT",
  "description": "DNB's answer to the question 'What tags belong into the <head> tag of my website?'",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com> (https://davids-neighbour.com)",
  "homepage": "https://github.com/dnb-org/dnb-hugo-testcontent#readme",
  "repository": "dnb-org/dnb-hugo-testcontent",
  "bugs": "https://github.com/dnb-org/dnb-hugo-testcontent/issues",
  "devDependencies": {
    "@dnb-org/remark-config": "^3.5.2",
    "@dnb-org/standard-version-config": "^3.5.1",
    "@dnb-org/textlint-config": "^3.5.2",
    "@dnb-org/tools": "^3.5.1"
  },
  "scripts": {
    "release": "standard-version --release-as patch -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release:major": "standard-version --release-as major -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release:minor": "standard-version --release-as minor -a -t \"v\" --releaseCommitMessageFormat \"chore(release): v{{currentTag}}\" && ./bin/release-hook-postrelease.sh"
  },
  "private": true,
  "remarkConfig": {
    "plugins": [
      "@dnb-org/remark-config"
    ]
  },
  "slug": "dnb-hugo-testcontent"
}
